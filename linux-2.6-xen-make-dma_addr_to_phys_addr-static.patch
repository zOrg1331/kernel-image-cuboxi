diff --git a/arch/i386/kernel/swiotlb.c b/arch/i386/kernel/swiotlb.c
index 0e73bb0..ae9e3b7 100644
--- a/arch/i386/kernel/swiotlb.c
+++ b/arch/i386/kernel/swiotlb.c
@@ -345,7 +345,7 @@ map_single(struct device *hwdev, struct phys_addr buffer, size_t size, int dir)
 	return dma_addr;
 }
 
-struct phys_addr dma_addr_to_phys_addr(char *dma_addr)
+static struct phys_addr dma_addr_to_phys_addr(char *dma_addr)
 {
 	int index = (dma_addr - iotlb_virt_start) >> IO_TLB_SHIFT;
 	struct phys_addr buffer = io_tlb_orig_addr[index];
